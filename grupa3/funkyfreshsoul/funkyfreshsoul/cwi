#include <iostream>
using namespace std;

int main() {
    int x, y;
    cin >> x >> y;
    if (x > 0 and y > 0)
        cout << "I" << endl;
    else if (x < 0 and y > 0)
        cout << "II" << endl;
    else if (x < 0 and y < 0)
        cout << "III" << endl;
    else if (x > 0 and y < 0)
        cout << "IV" << endl;
    else if (x = 0, y < 1000000000)
        cout << "0Y" << endl;
        else if (y = 0, x < 1000000000)
        cout << "0X" << endl;
    else
        cout << 0 << endl;
}
