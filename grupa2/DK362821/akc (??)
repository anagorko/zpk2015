#include <iostream>
using namespace std;

int akc( int t[], int n, int a ) {
    int k = n - 1, p = 0, s;
    while( p < k ) {
        s =( p + k + 1 ) / 2;
        if( t[ s ] <= a )
             p = s;
        else
             k = s - 1;
        
    }
    if( t[ k ] == a )
         return k + 1;
    else
         return 0;
    
}

int akc( int t[], int n, int a ) {
    int k = n - 1, p = 0, s;
    while( p < k ) {
        s =( k + p - 1 ) / 2;
        if( t[ s ] >= a )
             k = s;
        else
             p = s + 1;
        
    }
    if( t[ p ] == a )
         return p + 1;
    else
         return 0;
    
}

int main() {
    int n, m;
    cin >> n;
    int t[ n ];
    for( int i = 0; i < n; i++ )
         cin >> t[ i ];
    
    cin >> m;
    int u[ m ];
    for( int i = 0; i < m; i++ ) {
        cin >> u[ i ];
        int x = akc( t, n, u[ i ] );
        if( x == 0 )
             cout << 0 << endl;
        else {
            int y = akc( t, n, u[ i ] );
            cout << x - y + 1 << endl;
        }
    }
    return 0;
}
